{
  "name": "MCEMS API",
  "repository": "https://github.com/MCEMS/api",
  "env": {
    "ACTIVE911_REFRESH_TOKEN": {
      "description": "Your Active911 refresh token",
      "value": "DUMMY TOKEN"
    },
    "ACTIVE911_ALERT_EMAIL": {
      "description": "Your Active911 alert email address",
      "value": "example@example.com"
    },
    "SENDGRID_API_KEY": {
      "description": "API Key for SendGrid",
      "value": "DUMMY TOKEN"
    }
  },
  "buildpacks": [
    {
      "url": "https://github.com/strongloop/strongloop-buildpacks.git"
    }
  ],
  "addons": [
    "heroku-postgresql",
    "heroku-redis",
    "papertrail",
    "sendgrid"
  ]
}

